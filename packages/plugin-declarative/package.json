    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cam-inc/bento/issues"
  },
  "homepage": "https://github.com/cam-inc/bento#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@types/jest": "^28.1.4",
    "change-case": "^4.1.2",
