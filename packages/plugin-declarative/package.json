  "description": "Plugin for constructing Editor.js Tools.",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "run-p build test:unit test:integration test:smoke",
