{
  "name": "@bento-editor/element-paragraph",
  "version": "0.3.3",
  "description": "Paragraph Element for Bento Editor",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es.js",
      "require": "./dist/cjs.js"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "run-s clear build",
    "clear": "rimraf dist && mkdirp dist",
    "dev": "run-p dev:*",
    "dev:lib": "vite build --watch",
    "dev:type": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types --watch",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "test": "jest"
  },
  "homepage": "https://github.com/cam-inc/bento/tree/main/packages/element-paragraph",
  "bugs": {
    "url": "https://github.com/cam-inc/bento/issues"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Go Ohtani",
      "url": "https://github.com/cathcheeno"
    },
    {
      "name": "Shuta Hirai",
      "url": "https://github.com/shuta13"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cam-inc/bento.git"
  },
  "devDependencies": {
    "@bento-editor/jest-presets": "*",
    "@types/jest": "^28.1.1",
    "@vanilla-extract/vite-plugin": "^3.6.1",
    "@vitejs/plugin-react": "^2.1.0",
    "jest": "^28.1.1",
    "ts-node": "^10.8.1",
    "vite": "^3.1.8"
  },
  "peerDependencies": {
    "@bento-editor/core": "0.3.3"
  }
}
