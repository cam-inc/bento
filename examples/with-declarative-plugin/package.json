{
  "private": true,
  "name": "@bento-editor/with-declarative-plugin",
  "version": "0.0.1",
  "description": "With @bento-editor/declarative-plugin example.",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "node \"../../scripts/rmdir.js\" \"./dist\"",
    "build": "run-s typecheck build:prod",
    "build:dev": "webpack --mode=\"development\"",
    "build:prod": "webpack --mode=\"production\"",
    "typecheck": "tsc --noEmit",
    "watch": "npm run build:dev --watch",
    "serve": "budo --dir dist/ --live",
    "start": "run-s build serve",
    "start:watch": "run-p watch serve"
  },
